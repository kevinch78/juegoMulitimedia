{
  "buildCommand": "cd game-project && npm install && npm run build",
  "outputDirectory": "game-project/dist",
  "devCommand": "cd game-project && npm run dev",
  "installCommand": "cd game-project && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

